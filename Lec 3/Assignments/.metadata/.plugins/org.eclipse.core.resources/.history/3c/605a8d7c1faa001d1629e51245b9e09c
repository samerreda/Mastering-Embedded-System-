#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	int i,f;
	setbuf (stdout,NULL);
	puts("Factorial value"); /* prints Factorial value */

	printf("Enter an integer number: ");
	scanf("%d",&f);

	for(i=1;i<=f;i++)
		f*=i;
	printf("Factorial = %d",f);
	return EXIT_SUCCESS;
}
